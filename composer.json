{
  "name": "mimmi20/wurfl",
  "description": "the Wurfl PHP Library for PHP 5.3+",
  "type": "library",
  "license": "AGPLv3",
  "keywords": [
    "Wurfl",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/Wurfl",
  "require": {
    "php": ">=5.3.3",
    "psr/log": "^1.0",
    "mimmi20/wurflcache": "^1.3",
    "mimmi20/wurfl-generic-request": "dev-update-to-1.7.0",
    "mimmi20/wurfl-virtual-capability": "dev-update-to-1.7.0",
    "mimmi20/wurfl-config": "^1.0",
    "mimmi20/wurfl-storage": "^1.0",
    "mimmi20/wurfl-custom-device": "dev-update-to-1.7.0",
    "mimmi20/wurfl-constants": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "monolog/monolog": "^1.0",
    "mikey179/vfsStream": "^1.3",
    "squizlabs/php_codesniffer": "^2.0",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "suggest": {
    "monolog/monolog": "if you want to log to more than a file"
  },
  "autoload": {
    "psr-4": {
      "Wurfl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WurflTest\\": "tests/WurflTest/"
    }
  }
}
