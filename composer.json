{
  "name": "mimmi20/wurfl",
  "description": "the Wurfl PHP Library for PHP 5.3",
  "type": "library",
  "license": "LGPL-2.1",
  "keywords": [
    "Wurfl",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/Wurfl",
  "require": {
    "php": ">=5.3.3",
    "psr/log": "~1.0",
    "mimmi20/wurflcache": "~1.3",
    "mimmi20/wurfl-generic-request": "*",
    "mimmi20/wurfl-virtual-capability": "dev-update-to-version-1.6.1.0",
    "mimmi20/wurfl-config": "*",
    "mimmi20/wurfl-storage": "*",
    "mimmi20/wurfl-custom-device": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "monolog/monolog": "1.*@dev",
    "mikey179/vfsStream": "1.3.*"
  },
  "suggest": {
    "monolog/monolog": "if you want to log to more than a file"
  },
  "autoload": {
    "psr-4": {
      "Wurfl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WurflTest\\": "tests/WurflTest/"
    }
  }
}
