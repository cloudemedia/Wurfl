{
  "name": "mimmi20/wurfl",
  "description": "the Wurfl PHP Library for PHP 5.3",
  "type": "library",
  "license": "AGPLv3",
  "keywords": [
    "Wurfl",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/Wurfl",
  "require": {
    "php": ">=5.3.3",
    "psr/log": "~1.0",
    "mimmi20/wurflcache": "~1.3",
    "mimmi20/wurfl-generic-request": "*",
    "mimmi20/wurfl-virtual-capability": "~1.6.2.1",
    "mimmi20/wurfl-config": "~1.0.3",
    "mimmi20/wurfl-storage": "*",
    "mimmi20/wurfl-custom-device": "~1.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "monolog/monolog": "~1.13",
    "mikey179/vfsStream": "1.3.*",
    "squizlabs/php_codesniffer": "2.*",
    "fabpot/php-cs-fixer": "*"
  },
  "suggest": {
    "monolog/monolog": "if you want to log to more than a file"
  },
  "autoload": {
    "psr-4": {
      "Wurfl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WurflTest\\": "tests/WurflTest/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.6.2.1-dev"
    }
  }
}
