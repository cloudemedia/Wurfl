<?xml version="1.0" encoding="UTF-8"?>
<wurfl-config>
    <wurfl>
        <main-file>wurfl-regression.xml</main-file>
        <patches>
            <patch>web_browsers_patch.xml</patch>
            <patch>spv_patch.xml</patch>
            <patch>browsers.xml</patch>
        </patches>
    </wurfl>
    <!--
        <persistence> <provider>memcache</provider> </persistence>
    -->
    <allow-reload>true</allow-reload>
    <persistence>
        <provider>memory</provider>
        <params>host=127.0.0.1,port=11211</params>
    </persistence>

    <cache>
        <provider>null</provider>
        <params></params>
    </cache>
</wurfl-config>
